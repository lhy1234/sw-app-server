#spring.datasource.username=root
#spring.datasource.password=zhxs

#1
spring.jpa.hibernate.ddl-auto=update
#spring.jackson.serialization.indent_output=true

# MVC
spring.mvc.view.prefix=/page/
spring.mvc.view.suffix=.jsp
#pringboot的JPA由Hibernate实现。Springboot中默认配置的物理命名策略naming.physical-strategy为org.springframework.boot.orm.jpa.hibernate.SpringPhysicalNamingStrategy。
#把Identifier name用toUnderScoreCase下划线命名规则又转换一次
spring.jpa.hibernate.naming.physical-strategy=org.hibernate.boot.model.naming.PhysicalNamingStrategyStandardImpl
#shiro安全配置

shiro.loginUrl=/login
shiro.successUrl=/index
shiro.unauthorizedUrl=/unauthorized.jsp

shiro.globalSessionTimeout=1800000
shiro.validationInterval=1800000
shiro.cacheName=shiro-activeSessionCache
shiro.name=uid
shiro.validationSchedulerEnabled=true
shiro.deleteInvalidSessions=true
#shiro安全配置
shiro.domain=
shiro.path=/
shiro.httpOnly=true
shiro.maxAge=-1
shiro.rememeberMeName=rememberMe
shiro.rememeberMeMaxAge=2592000
shiro.rememeberCipherKey=4AvVhmFLUs0KTA3Kprsdag==
#系统管理员密码重试次数
shiro.maxRetryCount=10
#密码加密迭代次数
shiro.hashIterations=2
#密码加密算法
shiro.algorithmName=sha
#redis的服务器地址  
#redis.host=172.16.0.78
redis.host=127.0.0.1
#redis的服务端口 
redis.port=6379
#密码  
redis.pass=zhxs
#链接数据库  
redis.defaultDb=0
#客户端超时时间单位是毫秒  
redis.timeout=100000
#最大连接数 
redis.maxActive=300
#最大空闲数  
redis.maxIdle=100
#最小空闲数
redis.minIdle=10
#最大建立连接等待时间
redis.maxWait=1000
#明是否在从池中取出连接前进行检验,如果检验失败,则从池中去除连接并尝试取出另一个
redis.testOnBorrow=true
redis.testOnReturn=true


shiro.ip.whiteList=127.0.0.1,116.255.199.128
